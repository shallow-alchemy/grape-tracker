{
  "name": "mobile-app",
  "version": "1.0.0",
  "description": "Mobile-first web app with Zero sync",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"zero,backend,frontend\" -c \"cyan,magenta,green\" \"yarn dev:zero\" \"yarn dev:backend\" \"yarn dev:frontend\"",
    "dev:frontend": "rsbuild dev",
    "dev:backend": "cd backend && cargo run",
    "dev:zero": "npx zero-cache-dev --schema-path schema.js",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "zero-cache": "npx zero-cache-dev --schema-path schema.js",
    "backend": "cd backend && cargo run",
    "backend:watch": "cd backend && cargo watch -x run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rsbuild/core": "^1.6.1",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rsbuild/plugin-type-check": "^1.3.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.181.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.53.4",
    "@jscad/3mf-serializer": "^2.1.15",
    "@jscad/modeling": "^2.12.6",
    "@jscad/stl-serializer": "^2.1.21",
    "@rocicorp/zero": "^0.24.2025101500",
    "express": "^5.1.0",
    "jszip": "^3.10.1",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-qr-reader": "^3.0.0-beta-1",
    "three": "^0.181.0",
    "three-mesh-bvh": "^0.9.2",
    "three-stdlib": "^2.36.0",
    "wouter": "^3.7.1"
  }
}
